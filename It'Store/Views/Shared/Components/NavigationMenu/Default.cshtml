    </a>

    <ul style="background-color:#121212" class="dropdown-menu" aria-labelledby="dropdownMenuLink">
        @foreach (var q in Model)
        {
        <li>
            <a class="dropdown-item text-white"
               asp-action="Catalog"
               asp-controller="Catalog"
               asp-route-category="@q"
               asp-route-productPage="1">
                @q
            </a>
        </li>
        }
    </ul>
</div>